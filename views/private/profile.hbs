<div class="profileParent">
  <div class="editing-profile-container">
    <h1>Hi, {{user.username}}!</h1>
    <form action="/profile/{{_id}}" method="POST" enctype="multipart/form-data" class="editProfileForm">
      <div class="form-group">
        <img id="profile-img" src="{{user.userPhotoUrl}}" alt="User-img" />
        <input type="file" name="userPhotoUrl" />
      </div>

      <div class="form-group">
        <label for="exampleInputEmail1">Email</label>
        <input
          type="email"
          class="form-control"
          id="input-email"
          aria-describedby="emailHelp"
          value="{{user.email}}"
        />
        <small id="emailHelp" class="form-text text-muted">We'll never share
          your email with anyone else.</small>
      </div>

      <div class="form-group">
        <label for="usernamme">
          Username
          <input type="text" name="username" value="{{user.username}}" />
        </label>
      </div>

      <div class="form-group">
        <label for="age">
          Age
          <input type="text" name="age" value="{{user.age}}" />
        </label>
      </div>

      <div class="form-group">
        <label for="genre">
          Genre
          <input type="text" name="genre" value="{{user.genre}}" />
        </label>
      </div>

      <div class="form-group">
        <label for="country">
          <h4>Country</h4>
          <input type="text" name="country" value="{{user.country}}" />
        </label>
      </div>

      <div class="form-group">
        <label for="interests">
          <h4>Interests</h4>
          <select name="interests" id="interests" multiple>
            {{#each enumOptions}}
              <option value="{{this}}">{{this}}</option>
            {{/each}}
          </select>
        </label>
      </div>

      <div>
        <button type="submit" class="btn profileBtn">Update</button>
      </div>
    </form>

    <form>
      <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1" />
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
      </div>
      <div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </form>
  </div>
</div>